{
  "name": "eos",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "pretty": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "console": "node dist/console.js",
    "console:crawler": "ts-node -r tsconfig-paths/register src/console.ts crawler",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:create": "npm run typeorm migration:create -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "seed": "node ./dist/seed.js"
  },
  "dependencies": {
    "@automapper/core": "8.3.6",
    "@automapper/nestjs": "8.3.6",
    "@golevelup/nestjs-rabbitmq": "3.4.0",
    "@nestjs/axios": "0.0.7",
    "@nestjs/bull": "0.6.3",
    "@nestjs/common": "8.0.0",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.0.0",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/schedule": "2.0.1",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.0.3",
    "axios": "0.27.2",
    "bcrypt": "5.0.1",
    "bull": "4.10.4",
    "cheerio": "1.0.0-rc.11",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cors": "2.8.5",
    "cryptr": "6.2.0",
    "ethers": "6.3.0",
    "imei_gencheck": "4.0.0",
    "ioredis": "5.2.4",
    "jssoup": "0.0.15",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "mysql2": "2.3.3",
    "nestjs-console": "7.0.1",
    "passport": "0.5.3",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "qs": "6.11.0",
    "redis": "4.5.1",
    "request-ip": "2.1.3",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "swagger-ui-express": "4.4.0",
    "ts-md5": "1.2.11",
    "typeorm": "0.2.45",
    "utf8": "3.0.0",
    "web3": "1.9.0",
    "web3-utils": "1.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.0.0",
    "@nestjs/schematics": "8.0.0",
    "@nestjs/testing": "8.0.0",
    "@types/bull": "4.10.0",
    "@types/cron": "2.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "16.0.0",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/request-ip": "0.0.37",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "amqp-connection-manager": "4.1.10",
    "amqplib": "0.10.3",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.0.3",
    "prettier": "2.8.8",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
